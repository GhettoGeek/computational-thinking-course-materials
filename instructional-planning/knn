write "A trick for pretty printing our data (fewer decimal digits)"
Pretty = (key, val) ->
  if isNaN(val)
    return val
  else
    return val.toFixed(2)

write "Ex 1. Randomly split data into train and test sets"
trainTestSplit = (random ['train','test'],obs for obs in data)
train = (data[i] for i in [0..data.length-1] when trainTestSplit[i] is 'train')
test = (data[i] for i in [0..data.length-1] when trainTestSplit[i] is 'test')
write "trainTestSplit[0..10] is:" + JSON.stringify(trainTestSplit[0..10],Pretty)
write train.length + " in train. train[0] is:" + JSON.stringify(train[0],Pretty)
write test.length + " in test. test[0] is:" + JSON.stringify(test[0],Pretty)

write "Ex 2. Find the closest known observations in train"
KNN = (testObs,neighbors) ->
  scores = []
  for trainObs in train
    score = 0
    for k,v of testObs when not isNaN(v)
      score += Math.pow(v-trainObs[k],2)
    score = Math.sqrt(score)
    scores.push( {score:score,class:trainObs.class} )
  scores.sort (a, b) -> a.score - b.score
  return scores[0..neighbors]
write "Do majority voting"
Majority = (scores) ->
  frequencies = {}
  for s in scores 
    frequencies[s.class] = (frequencies[s.class] ? 0) + 1;
  sortedFrequencies = ( {class: c, frequency:f} for c,f of frequencies ).sort (a, b) -> b.frequency - a.frequency
  return sortedFrequencies[0].class
  
write "Ex 3. Test example"
testObs = random test
result = KNN(testObs,6)
write "Testing: " + testObs.class
write "KNN: " + JSON.stringify(result,Pretty)
write "Majority: " + Majority(result)

write "Ex 4. Test synthetic example"
testObs = {"Order4Length":72,"Order3Length":23,"class":"L.Benzoin"}
result = KNN(testObs,3)
write "Testing: " + testObs.class
write "KNN: " + JSON.stringify(result,Pretty)
write "Majority: " + Majority(result)

write "Test everything in the test set"
TestAll = (data,neighbors) ->
  right = 0
  wrong = 0
  for testObs in data
    knnScores = KNN(testObs,neighbors)
    majorityClass = Majority(knnScores)
    if testObs.class is majorityClass
      right++
    else
      wrong++
  percentRight = right / (right+wrong)
  return percentRight
write "Ex 5. Check accuracy for different neighbors"
speed 100
[xOffset,yOffset] = [-400,-300]
turtle.jumpto xOffset,yOffset
turtle.pen blue, 1
lt 45
hardTest = (  {Order4Length:obs.Order4Length,class:obs.class}  for obs in test )
mediumTest = (  {Order4Length:obs.Order4Length, Order3Length:obs.Order3Length,class:obs.class}  for obs in test )
easyTest = test
for neighbors in [1..50] 
  percentRight = TestAll(train,neighbors)
  turtle.moveto neighbors*20+xOffset,percentRight*400+yOffset
  label neighbors + ":" + percentRight.toFixed(2)

write "Ex 6. Show random baseline"
randomBaseline = new Turtle()
randomBaseline.pen red, 1
randomBaseline.jumpto xOffset,.25*400+yOffset
randomBaseline.moveto 1000+xOffset,.25*400+yOffset

